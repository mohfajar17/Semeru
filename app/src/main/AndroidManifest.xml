        <activity android:name=".WholesaleActivity" />
        <activity android:name=".EnterpriseActivity" />
        <activity android:name=".GovernmentActivity" />
        <activity android:name=".BussinesActivity" />
        <activity android:name=".GlobalActivity" />
        <activity android:name=".UnderspecActivity" />
        <activity android:name=".VoiceActivity" />
        <activity android:name=".TelinActivity" />
        <activity android:name=".GamasFeederActivity" />
        <activity android:name=".TransportActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SplashActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name=".AppReceiver"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>